h1 {
    text-align: center
}
h2##{aDomId} {
    color: #990
}
img {
  max-width: 100%;
}

textarea, #editor div {
  display: inline-block;
  width: 100%;
  height: 100%;
  vertical-align: top;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

textarea {
  border: none;
  resize: none;
  outline: none;
  background-color: #f6f6f6;
  font-size: 14px;
  padding: 20px;
}

div #preview {
  width: 100%;
  display: none;
}
